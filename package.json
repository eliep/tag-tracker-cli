{
  "name": "tag-tracker-cli",
  "version": "0.3.4",
  "description": "Tag your days",
  "main": "build/tag.js",
  "scripts": {
    "build": "tsc && sed -i 's/@VERSION@/'$npm_package_version'/' build/tag.js",
    "test": "jest",
    "testCoverage": "jest --coverage",
    "run": "tag --help",
    "release": "bin/release.sh",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "bin": {
    "tag": "build/tag.js"
  },
  "keywords": [
    "timetracker",
    "cli",
    "tag"
  ],
  "author": "eprudhomme@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliep/tag-tracker-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "cli-color": "^1.3.0",
    "clui": "^0.3.6",
    "commander": "^2.19.0",
    "configstore": "^4.0.0",
    "csv": "^4.0.0",
    "deepmerge": "^2.2.1",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/configstore": "^2.1.1",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "vuepress": "^0.14.4"
  }
